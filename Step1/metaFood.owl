#################################################################
#    Object Properties
#################################################################

###  http://metafood.org#acceptedPaymentCashMethod
food:acceptedPaymentCashMethod rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf gr:acceptedPaymentMethods ;
                               rdfs:domain food:OrderSpecification ;
                               rdfs:range food:PaymentMethodCash ;
                               owl:propertyDisjointWith food:hasCurrency ;
                               rdfs:comment "The sub property of gr:acceptedPaymentMethods represents food:PaymentMethodCash accepted for an order." ;
                               rdfs:label "accepted payment cash method" .


###  http://metafood.org#acceptedPaymentCreditMethod
food:acceptedPaymentCreditMethod rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf gr:acceptedPaymentMethods ;
                                 rdfs:domain food:OrderSpecification ;
                                 rdfs:range gr:PaymentMethodCreditCard ;
                                 owl:propertyDisjointWith food:hasCurrency ;
                                 rdfs:comment "The sub property of gr:acceptedPaymentMethods represents gr:acceptedPaymentCreditMethod accepted for an order.." ;
                                 rdfs:label "accepted payment credit method" .


###  http://metafood.org#hasContactSpecification
food:hasContactSpecification rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             rdfs:domain gr:BusinessEntity ;
                             rdfs:range ic:iContactThing ;
                             rdfs:comment "The address or phone numer that a buisiness entity can have." ;
                             rdfs:label "has contact specification" .


###  http://metafood.org#hasCuisine
food:hasCuisine rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain food:Restaurant ;
                rdfs:range food:Cuisine ;
                owl:propertyDisjointWith food:hasCurrency ;
                rdfs:comment "hasCuisine links the food:Restaurant to the food:Cuisine class." ;
                rdfs:label "has cuisine" .


###  http://metafood.org#hasCurrency
food:hasCurrency rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain food:Price ;
                 rdfs:range food:Currency ;
                 owl:propertyDisjointWith ic:hasAddress ;
                 rdfs:comment "hasCurrency links the food:Price class to the food:Currency class." ;
                 rdfs:label "has currency" .


###  http://metafood.org#hasDeliverySpecification
food:hasDeliverySpecification rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdfs:domain food:OrderSpecification ;
                              rdfs:range food:DeliverySpecification ;
                              rdfs:comment "hasDeliverySpecification links food:OrderSpecification class to the food:DeliverySpecification class." ;
                              rdfs:label "has delivery specification" .


###  http://metafood.org#hasFoodItem
food:hasFoodItem rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain <http://purl.org/ontology/fo/Menu> ;
                 rdfs:range <http://purl.org/ontology/fo/Food> ;
                 rdfs:comment "hasFoodItem links fo:Menu class to the fo:Food class." ;
                 rdfs:label "has food item" .


###  http://metafood.org#hasOrderSpecification
food:hasOrderSpecification rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           owl:inverseOf food:orderSpecificationOfRestaurant ;
                           rdfs:domain food:Restaurant ;
                           rdfs:range food:OrderSpecification ;
                           rdfs:comment "hasOrderSpecification links food:Restaurant class to the food:OrderSpecification class." ;
                           rdfs:label "has order specification" .


###  http://metafood.org#hasPhoneNumber
food:hasPhoneNumber rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf food:hasContactSpecification ;
                    rdfs:domain food:Restaurant ;
                    rdfs:range ic:PhoneNumber ;
                    rdfs:comment "hasPhoneNumber links any class to the food:PhoneNumber class." ;
                    rdfs:label "has phone number" .


###  http://metafood.org#hasPrice
food:hasPrice rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf food:isPriceOf ;
              rdfs:domain <http://purl.org/ontology/fo/Menu> ;
              rdfs:range food:Price ;
              rdfs:comment "hasPrice links any class to the food:Price class." ;
              rdfs:label "has price" .


###  http://metafood.org#hasRating
food:hasRating rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain food:Restaurant ;
               rdfs:range food:Rating ;
               rdfs:comment "hasRating links food:Restaurant class to food:Rating class." ;
               rdfs:label "has rating" .


###  http://metafood.org#isDeliveredBy
food:isDeliveredBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain food:OrderSpecification ;
                   rdfs:range food:DeliveryService ;
                   rdfs:comment "isDeliveredBy links food:OrderSpecification class to the food:DeliveryService class." ;
                   rdfs:label "is delivered by" .


###  http://metafood.org#isPriceOf
food:isPriceOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain food:Price ;
               rdfs:range <http://purl.org/ontology/fo/Menu> ;
               rdfs:comment "isPriceOf links the food:Price class to any class." ;
               rdfs:label "is price of" .


###  http://metafood.org#orderSpecificationOfRestaurant
food:orderSpecificationOfRestaurant rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf owl:topObjectProperty ;
                                    rdfs:domain food:OrderSpecification ;
                                    rdfs:range food:Restaurant ;
                                    rdfs:comment "isOrderSpecificationOfRestaurant links food:OrderSpecification class to the food:Restaurant class." ;
                                    rdfs:label "is order specificatio of restaurant" .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasAddress
ic:hasAddress rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf food:hasContactSpecification ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain food:Restaurant ;
              rdfs:range ic:Address ;
              rdfs:comment "hasAddress links any class to the ic:Address class."@en ;
              rdfs:label "has address" .


###  http://purl.org/goodrelations/v1#acceptedPaymentMethods
gr:acceptedPaymentMethods rdf:type owl:ObjectProperty ;
                          rdfs:domain food:OrderSpecification ;
                          rdfs:range gr:PaymentMethod ;
                          rdfs:comment "The gr:PaymentMethod or methods accepted by the gr:BusinessEntity."@en ;
                          rdfs:isDefinedBy <http://purl.org/goodrelations/v1> ;
                          rdfs:label "accepted payment methods"@en .


###  http://purl.org/goodrelations/v1#offers
gr:offers rdf:type owl:ObjectProperty ;
          rdfs:domain food:Restaurant ;
          rdfs:range <http://purl.org/ontology/fo/Menu> ;
          rdfs:comment "This links a food:Restaurant to the foods fo:Food it offers."@en ;
          rdfs:isDefinedBy <http://purl.org/goodrelations/v1> ;
          rdfs:label "offers"@en .


#################################################################
#    Data properties
#################################################################

###  http://metafood.org#hasAreaCode
food:hasAreaCode rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf food:hasTelephoneData ;
                 rdfs:domain ic:PhoneNumber ;
                 rdfs:range xsd:int ;
                 rdfs:comment "a series of numbers used before the main phone number when someone phone someone outside a own town or area." ;
                 rdfs:label "has area code" .


###  http://metafood.org#hasCity
food:hasCity rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf ic:addressDataProperty ;
             rdfs:domain ic:Address ;
             rdfs:range xsd:string ;
             rdfs:comment "the name of a city." ;
             rdfs:label "has city" .


###  http://metafood.org#hasCountryCode
food:hasCountryCode rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf food:hasTelephoneData ;
                    rdfs:domain ic:PhoneNumber ;
                    rdfs:range xsd:int ;
                    rdfs:comment "a series of numbers used before the main phone number when someone phone someone outside a country." ;
                    rdfs:label "has country code" .


###  http://metafood.org#hasCurrencyValue
food:hasCurrencyValue rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf food:hasPriceData ;
                      rdfs:domain food:Currency ;
                      rdfs:range xsd:string ;
                      rdfs:comment "The value that a currency  of a price can take, such as EUR, GBP etc." ;
                      rdfs:label "has currency value" .


###  http://metafood.org#hasDeliveryCharge
food:hasDeliveryCharge rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain food:DeliverySpecification ;
                       rdfs:range xsd:float ;
                       rdfs:comment "is the amount of money it takes for a delivery service to deliver the order." ;
                       rdfs:label "has delivery charge" .


###  http://metafood.org#hasDeliveryTime
food:hasDeliveryTime rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain food:DeliverySpecification ;
                     rdfs:range xsd:int ;
                     rdfs:comment "typical delivery time to an address, that it takes for a delivery service to deliver the order." ;
                     rdfs:label "has delivery time" .


###  http://metafood.org#hasLineNumber
food:hasLineNumber rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf food:hasTelephoneData ;
                   rdfs:domain ic:PhoneNumber ;
                   rdfs:range xsd:int ;
                   rdfs:comment "a series of numbers used as the main phone number when someone phone someone within a town or area." ;
                   rdfs:label "has line number" .


###  http://metafood.org#hasMinOrder
food:hasMinOrder rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain food:OrderSpecification ;
                 rdfs:range xsd:float ;
                 rdfs:comment "min amount of an order, in pounds." ;
                 rdfs:label "has min order" .


###  http://metafood.org#hasPriceCategory
food:hasPriceCategory rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain food:Restaurant ;
                      rdfs:range xsd:string ;
                      rdfs:comment """It specifies how expensive the food offered by a restaurant according to customers experiences.
Example: €, €€, €€€. In this case €€ would mean, the price is fair and not over budget.""" ;
                      rdfs:label "has price category" .


###  http://metafood.org#hasPriceData
food:hasPriceData rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:comment "generalized property for price data properties." ;
                  rdfs:label "has price data" .


###  http://metafood.org#hasPriceValue
food:hasPriceValue rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf food:hasPriceData ;
                   rdfs:domain food:Price ;
                   rdfs:range xsd:float ;
                   rdfs:comment "A value that the price of a menu can take." ;
                   rdfs:label "has price value" .


###  http://metafood.org#hasRatingCount
food:hasRatingCount rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain food:Rating ;
                    rdfs:range xsd:int ;
                    rdfs:comment "how many users has rated a restaurant on the delivery service website." ;
                    rdfs:label "has rating count" .


###  http://metafood.org#hasRatingValue
food:hasRatingValue rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain food:Rating ;
                    rdfs:range xsd:float ;
                    rdfs:comment """It takes values between 0 and 5 to evaluate a restaurant on the delivery service website.
Example: 5 corresponds to high rated restaurant.""" ;
                    rdfs:label "has rating value" .


###  http://metafood.org#hasTelephoneData
food:hasTelephoneData rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf ic:hasContactDataProperty ;
                      rdfs:domain ic:PhoneNumber ;
                      rdfs:comment "generalized property for telephone data properties." ;
                      rdfs:label "has telephone data" .


###  http://metafood.org#hasWaitingTime
food:hasWaitingTime rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain food:OrderSpecification ;
                    rdfs:range xsd:int ;
                    rdfs:comment "how long it takes an order after is picked up to be ready for delivery." ;
                    rdfs:label "has waiting time" .


###  http://metafood.org#isEnabled
food:isEnabled rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain gr:PaymentMethod ;
               rdfs:range xsd:string ;
               rdfs:comment "It specifies whether payment method for a restaurant is enabled." ;
               rdfs:label "is enabled" .


###  http://metafood.org#isOffered
food:isOffered rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain <http://purl.org/ontology/fo/Food> ;
               rdfs:range xsd:boolean ;
               rdfs:comment "It specifies whether breakfast, lunch or snacks are offered by a restaurant or not." ;
               rdfs:label "is offered" .


###  http://ontology.eil.utoronto.ca/icontact.owl#addressDataProperty
ic:addressDataProperty rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf ic:hasContactDataProperty ;
                       rdfs:comment "generalized property for address data properties." ;
                       rdfs:label "address data property" .


###  http://ontology.eil.utoronto.ca/icontact.owl#emailDataProperty
ic:emailDataProperty rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf ic:hasContactDataProperty ;
                     rdfs:domain food:Restaurant ;
                     rdfs:range xsd:string ;
                     rdfs:comment "an email address of a restaurant." ;
                     rdfs:label "email data property" .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasBuilding
ic:hasBuilding rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf ic:addressDataProperty ;
               rdfs:domain ic:Address ;
               rdfs:range xsd:string ;
               rdfs:comment "The building where the address is located.  Often used in countries like England, India and Japan."@en ;
               rdfs:label "has building"@en .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasContactDataProperty
ic:hasContactDataProperty rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty ;
                          rdfs:comment "generalized property for contact data properties." ;
                          rdfs:label "has contact data property" .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasPostalCode
ic:hasPostalCode rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf ic:addressDataProperty ;
                 rdfs:domain food:ThingsWithPostalCode ;
                 rdfs:range xsd:string ;
                 rdfs:comment "postal code of an adress. It can be also delivery postal code." ;
                 rdfs:label "has postal code"@en .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasStreet
ic:hasStreet rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf ic:addressDataProperty ;
             rdfs:domain ic:Address ;
             rdfs:range xsd:string ;
             rdfs:comment "The name of a street of an adress." ;
             rdfs:label "has street"@en .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasStreetNumber
ic:hasStreetNumber rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf ic:addressDataProperty ;
                   rdfs:domain ic:Address ;
                   rdfs:range xsd:string ;
                   rdfs:comment "The number of a house." ;
                   rdfs:label "has street number" .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasUnitNumber
ic:hasUnitNumber rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf ic:addressDataProperty ;
                 rdfs:domain ic:Address ;
                 rdfs:range xsd:int ;
                 rdfs:comment "add on to house number, like 56/2" ;
                 rdfs:label "has unit number"@en .


###  http://ontology.eil.utoronto.ca/icontact.owl#hasWebSite
ic:hasWebSite rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf ic:hasContactDataProperty ;
              rdfs:domain food:ThingsWithWebsite ;
              rdfs:range xsd:string ;
              rdfs:comment "the website URL of a restaurant." ;
              rdfs:label "has web site"@en .


###  http://purl.org/goodrelations/v1#description
gr:description rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain food:ThingsWithDescription ;
               rdfs:range xsd:string ;
               rdfs:comment """A short textual description of the resource. 

This property is semantically equivalent to rdfs:comment and just meant as a handy shortcut for marking up data.""" ;
               rdfs:label "has description" .


###  http://purl.org/goodrelations/v1#legalName
gr:legalName rdf:type owl:DatatypeProperty ;
             rdfs:domain food:ThingsWithName ;
             rdfs:range xsd:string ;
             rdfs:comment "The name of the gr:BusinessEntity or fo:Menu."@en ;
             rdfs:isDefinedBy <http://purl.org/goodrelations/v1> ;
             rdfs:label "has name" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
<http://www.w3.org/2003/01/geo/wgs84_pos#lat> rdf:type owl:DatatypeProperty ;
                                              rdfs:subPropertyOf ic:addressDataProperty ;
                                              rdfs:domain ic:Address ;
                                              rdfs:range xsd:float ;
                                              rdfs:comment "a unit that represents a coordinate at geographic coordinate system." ;
                                              rdfs:label "lattitude"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
<http://www.w3.org/2003/01/geo/wgs84_pos#long> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf ic:addressDataProperty ;
                                               rdfs:domain ic:Address ;
                                               rdfs:range xsd:float ;
                                               rdfs:comment "a unit that represents a coordinate at geographic coordinate system." ;
                                               rdfs:label "longitude"@en .


#################################################################
#    Classes
#################################################################

###  http://metafood.org#Breakfast
food:Breakfast rdf:type owl:Class ;
               rdfs:subClassOf <http://purl.org/ontology/fo/Food> ;
               rdfs:comment "The subclass of fo:Food represents the first meal of the day that is by a restaurant." ;
               rdfs:label "Breakfast" .


###  http://metafood.org#ChineseRestaurant
food:ChineseRestaurant rdf:type owl:Class ;
                       rdfs:subClassOf food:Restaurant ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty food:hasCuisine ;
                                         owl:hasValue food:chinese
                                       ] ;
                       rdfs:comment "It represents a restaurant that prepares and serves Chinese food to customers." ;
                       rdfs:label "Chinese restaurant" .


###  http://metafood.org#Cuisine
food:Cuisine rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( food:burgers
                                               food:chinese
                                               food:curry
                                               food:halal
                                               food:indian
                                               food:italian
                                               food:japanese
                                               food:noodles
                                               food:pasta
                                               food:pizza
                                             )
                                 ] ;
             rdfs:subClassOf food:ThingsWithName ;
             rdfs:comment "A cuisine is a style of cooking characterized by distinctive ingredients, techniques and dishes, and usually associated with a specific culture or geographic region." ;
             rdfs:label "Cuisine" .


###  http://metafood.org#Currency
food:Currency rdf:type owl:Class ;
              owl:disjointWith food:Price ;
              rdfs:comment "A system of money in general use in a particular country." ;
              rdfs:label "Currecny" .


###  http://metafood.org#DeliveryService
food:DeliveryService rdf:type owl:Class ;
                     rdfs:subClassOf gr:BusinessEntity ;
                     owl:hasKey ( gr:legalName
                                ) ;
                     rdfs:comment "a courier service in which a restaurant, store, or independent food delivery company delivers food to a customer." ;
                     rdfs:label "Delivery service" .


###  http://metafood.org#DeliverySpecification
food:DeliverySpecification rdf:type owl:Class ;
                           rdfs:subClassOf food:ThingsWithPostalCode ,
                                           owl:Thing ;
                           rdfs:comment """This class represents characteristics relates to a delivery service offered by a restaurant.
Examples: delivery charge, delivery time""" ;
                           rdfs:label "Delivery specification" .


###  http://metafood.org#FastFoodRestaurant
food:FastFoodRestaurant rdf:type owl:Class ;
                        rdfs:subClassOf food:Restaurant ,
